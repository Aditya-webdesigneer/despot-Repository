        // Get all table cells
        var cells = document.querySelectorAll('td');

        // Add click event listener to each cell
        cells.forEach(function(cell) {
            cell.addEventListener('click', function() {
                // Retrieve element information
                var atomicNumber = this.textContent.trim().split('\n')[0];
                var elementName = this.querySelector('abbr').getAttribute('title');
                var symbol = this.querySelector('abbr').textContent.trim();
                var atomicMass = this.getAttribute('data-mass');
                var magneticProperty = this.getAttribute('data-magnetic');
                var state = this.getAttribute('data-state');
                var superconductivity = superconductivityData[symbol] || "Unknown"; // Retrieve superconductivity data

               // Construct the information string
                var elementInfo = "Atomic Number: " + atomicNumber + "<br>" +
                    "Element Name: " + elementName + "<br>" +
                    "Symbol: " + symbol + "<br>" +
                    "Atomic Mass: " + atomicMass + "<br>" +
                    "Magnetic Property: " + magneticProperty + "<br>" +        
                    "State: " + state + "<br>"+
                    "Superconductivity: " + superconductivity + "<br>";


                // Display element information
                document.getElementById('elementInfo').innerHTML = "<h2>" + elementInfo + "</h2>";
            });
        });
    </script>
</body>
 </script>
        <div style="text-align:left; margine-lef:10px;">
    <p>.Aditya Deshpande</p>

    </script>
</html>
</body>

</html>
